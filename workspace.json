{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "api",
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/api/**/*.ts"]
          }
        },
        "codegen": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "npx prisma generate --schema apps/api/prisma/schema.prisma"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        },
        "integration": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api-int"],
          "options": {
            "jestConfig": "apps/api/jest.integration.config.js",
            "runInBand": true
          }
        },
        "db-migrate": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn db:migrate"
          }
        },
        "db-deploy": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/api",
            "command": "npx prisma migrate deploy --preview-feature"
          }
        },
        "db-reset": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn db:migrate"
          }
        },
        "docker.up": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/api",
            "command": "docker-compose up -d"
          }
        },
        "docker.down": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "cwd": "apps/api",
            "command": "docker-compose down"
          }
        }
      }
    },
    "users": {
      "root": "libs/users",
      "sourceRoot": "libs/users/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/users/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/users"],
          "options": {
            "jestConfig": "libs/users/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/utils/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/utils"],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
